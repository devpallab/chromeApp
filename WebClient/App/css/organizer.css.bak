.lesson_button_container.swiper-slide{ padding:0 4px;     height:105px !important;}
.lesson_button_container.swiper-slide .week-no {
    width: 8%;
    float: none;
    border: 2px solid #fff;
    border-radius: 4px;
    overflow: hidden;
    background-color: #c0c0c0;
    background-image: linear-gradient(transparent 50%, rgba(255,255,255,.5) 10%);
    background-size: 2px 2px;
    background-color: #c0c0c0;
    background-image: linear-gradient(transparent 75%, rgba(255,255,255,.5) 10%);
    background-size: 2px 2px;
    margin: 15px 10px 0px 0 !important;
	-webkit-transition: all 200ms ease-in;
    -webkit-transform: scale(1); 
    -ms-transition: all 200ms ease-in;
    -ms-transform: scale(1); 
    -moz-transition: all 200ms ease-in;
    -moz-transform: scale(1);
    transition: all 200ms ease-in;
    transform: scale(1); 
	display: inline-block;
    vertical-align: top;
	cursor : pointer;	
}
button.btn-normal2 {
    font-size: 10px;
    padding: 4px 10px;
}
.outside_organize_wrap .outside_tabreading__Notes #sList li { padding-right:0;}
.outside_organize_wrap .outside_tabreading__Notes  #sList .selected button.btn-normal2, .outside_organize_wrap .outside_tabreading__Notes  #sList :hover button.btn-normal2 {
        background-color: #000;
    color: #fff;
}
.outside_organize_wrap .outside_tabreading__Notes  #sList .selected , .outside_organize_wrap .outside_tabreading__Notes  #sList :hover  {
        background-color:transparent;
    color: inherit;
}


.lesson_button_container.swiper-slide .week-no.weekselected {
    z-index: 2;
    transition: all 200ms ease-in;
    transform: scale(1.1);
    height: 97px;
    width: 8%;
    margin-top: 5px !important;
    /* border-color: #0073BC; */
    background-image: none; border-width:2px;
    /* background-color: #0073BC !important; */
}
